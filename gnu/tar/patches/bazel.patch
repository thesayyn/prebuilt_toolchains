diff --git a/config/gen.malloc.awk b/config/gen.malloc.awk
new file mode 100644
index 0000000..05fbdbe
--- /dev/null
+++ b/config/gen.malloc.awk
@@ -0,0 +1,35 @@
+
+BEGIN {
+    print "SUBSTITUTIONS = {";
+    within_gl = 0;
+}
+{
+  if (within_gl && $0 == "") {
+    within_gl = 0
+    print "\t},"
+    next;
+  } 
+
+  if (within_gl && $1 == "-e") {
+    if ($2 ~ "d'$") {
+      print "\t\t\"" substr($2, 3, length($2) - 5) "\":"  " \"//\","
+      next;
+    }
+
+    split($0,  a, "|")
+    gsub("\\\\.", ".", a[2])
+    print "\t\t\"" a[2] "\":" "\"" a[3] "\","
+    next;
+    
+
+  }
+  if ($1 ~ "^#" || $1 !~ "gl.h:$" ) {
+    next;
+  }
+  print "\t\"" $2 "\": {"
+  within_gl = 1
+}
+END {
+
+    print "}";
+}
\ No newline at end of file
