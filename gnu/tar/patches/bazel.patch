diff --git a/config/gen.gnu.awk b/config/gen.gnu.awk
new file mode 100644
index 0000000..fb4cf3d
--- /dev/null
+++ b/config/gen.gnu.awk
@@ -0,0 +1,38 @@
+BEGIN {
+    print "SUBSTITUTIONS = {";
+}
+{
+  if ($2 != "=") {
+    next;
+  }
+  if ($1 != toupper($1)) {
+    next;
+  }
+  if ($3 ~ "noinst_LIBRARIES") {
+    next;
+  }
+  gsub("GL_", "");
+  st = index($0,"=");
+  val = substr($0,st+2)
+  gsub("\"", "\\\\\"", val);
+  if (index(val, "\\") > 0 || index(val, "$") > 0 ) {
+    next;
+  }
+  a[$1]++;
+  if (a[$1] > 1) {
+    next;
+  }
+  ORS = "";
+  print "\t\"@" $1 "@\":"
+  if (length(val) == 0) {
+    print "\"\"";
+  } else {
+    print "\"\"\"" val "\"\"\"";
+  }
+  ORS = "\n";
+  print ",";
+}
+END {
+    print "\t\"@GUARD_PREFIX@\":\"\"\"GL\"\"\",";
+    print "}";
+}
