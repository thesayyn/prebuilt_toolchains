diff --git a/config/expand_hdr.bzl b/config/expand_hdr.bzl
new file mode 100644
index 0000000..8a59d26
--- /dev/null
+++ b/config/expand_hdr.bzl
@@ -0,0 +1,61 @@
+"expand_hdr rule"
+
+def _expand_hdr_impl(ctx):
+    jq_bin = ctx.toolchains["@aspect_bazel_lib//lib:jq_toolchain_type"].jqinfo.bin
+    expand_template_info = ctx.toolchains["@aspect_bazel_lib//lib:expand_template_toolchain_type"].expand_template_info
+
+    replacements_out = ctx.actions.declare_file(ctx.label.name + ".replacements.json")
+
+    args = ctx.actions.args()
+    args.add("""-n""")
+    args.add("{} + $ARGS.named".format(json.encode(ctx.attr.substitutions)))
+    for repl in ctx.attr.replacements:
+        rwith = ctx.attr.replacements[repl][DefaultInfo].files
+        args.add("--rawfile")
+        args.add(repl, format = '%s')
+        args.add_all(rwith)
+ 
+    ctx.actions.run_shell(
+        arguments = [args],
+        outputs = [replacements_out],
+        inputs = ctx.files.replacements,
+        command = '{} "$@" > {}'.format(jq_bin.path, replacements_out.path),
+        tools = [jq_bin],
+        toolchain = "@aspect_bazel_lib//lib:jq_toolchain_type",
+    )
+    
+    args = ctx.actions.args()
+    args.add(ctx.file.hdr)
+    args.add(ctx.outputs.out)
+    args.add(replacements_out)
+    args.add("/dev/null")
+    args.add("/dev/null")
+    args.add("false")
+
+    ctx.actions.run(
+        arguments = [args],
+        outputs = [ctx.outputs.out],
+        inputs = [
+            ctx.file.hdr,
+            replacements_out,
+        ],
+        executable = expand_template_info.bin,
+        toolchain = "@aspect_bazel_lib//lib:expand_template_toolchain_type",
+    )
+
+    return [DefaultInfo(files = depset([ctx.outputs.out]))]
+
+
+expand_hdr = rule(
+    implementation = _expand_hdr_impl,
+    toolchains = [
+        "@aspect_bazel_lib//lib:expand_template_toolchain_type", 
+        "@aspect_bazel_lib//lib:jq_toolchain_type"
+    ],
+    attrs = {
+        "hdr": attr.label(mandatory = True, allow_single_file = True),
+        "out": attr.output(mandatory = True),
+        "substitutions": attr.string_dict(),
+        "replacements": attr.string_keyed_label_dict(allow_files = True),
+    },
+)
